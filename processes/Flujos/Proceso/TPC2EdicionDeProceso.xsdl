}

this.form.listaPedidos.clear();
for (int i = 0; i &amp;lt; this.form.objeto.pedidos.length; i++) {
    this.form.listaPedidos.extend(objetoPedido : this.form.objeto.pedidos[i]);
}

this.form.listaComentarios.clear();
for (int i = 0; i &amp;lt; this.form.objeto.comentarios.length; i++) {

