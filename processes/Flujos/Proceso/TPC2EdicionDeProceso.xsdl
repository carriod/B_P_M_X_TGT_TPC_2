	}
	
	if(this.rol == &amp;quot;TPC - Supervisor CdG&amp;quot; || this.rol == &amp;quot;TPC - Analista CdG&amp;quot; || this.rol == &amp;quot;TPC2 - Consulta&amp;quot; || this.rol == &amp;quot;TPC - Solicitante SAGI&amp;quot;){
		if(objComentario.tipo != &amp;quot;Observación&amp;quot;){
