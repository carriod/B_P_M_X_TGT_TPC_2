		&lt;method name=&quot;obtenerProcesos&quot;
			flags=&quot;public server&quot;&gt;
			&lt;sourceCode
				language=&quot;Java1&quot;&gt;Respuesta respuesta;
this.form.listadoProcesos.clear();

respuesta = FProceso2.getValuesForFiltros(proceso : this.form.objeto, 
										  diasEstado : this.form.diasEstado, 
										  diasCompra : this.form.diasCompra, 
										  fechaIngresoDesde : this.form.fechaIngresoDesde, 
                              			  fechaIngresoHasta : this.form.fechaIngresoHasta, 
                              			  fechaObjetivoDesde : this.form.fechaObjetivoDesde, 
                              			  fechaObjetivoHasta : this.form.fechaObjetivoHasta);
if(respuesta.resultado == OK){	
	for(NProceso2 proceso: (NProceso2[])respuesta.contenido){
		this.form.listadoProcesos.extend(objeto: proceso, listaPrioridad: proceso.prioridad);
	}
}else{
	Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
	throw Java.Lang.Exception((String)respuesta.contenido);
}

//BOTONES DE ACCION
this.form.botonesAccion.clear();
this.form.botonesAccion.extend(labelBoton : &amp;quot;Cancelar&amp;quot;);
this.form.botonesAccion.extend(labelBoton : &amp;quot;Volver&amp;quot;);
this.form.botonesAccion.extend(labelBoton : null);
this.form.botonesAccion.extend(labelBoton : null);
this.form.botonesAccion.extend(labelBoton : &amp;quot;CTA&amp;quot;);
this.form.botonesAccion.extend(labelBoton : &amp;quot;DJAI&amp;quot;);
this.form.botonesAccion.extend(labelBoton : &amp;quot;Completo&amp;quot;);
this.form.botonesAccion.extend(labelBoton : &amp;quot;Acotado&amp;quot;);&lt;/sourceCode&gt;
