			&lt;sourceCode
				language=&quot;Java1&quot;&gt;Respuesta respuesta;
Any[][] datos = {};
Integer[] solicitudes = {};

if(this.result == &amp;quot;acotado&amp;quot;){
	FConfiguracion2.cargarTitulosReporteProceso(datos : datos, out datos : datos);
	
	foreach (objeto in this.form.listadoProcesos){
		FConfiguracion2.cargarContenidoReportesProceso(proceso: objeto.objeto, datos: datos, out datos: datos);
	}
	
	
}else if(this.result == &amp;quot;completo&amp;quot;){
	FConfiguracion2.cargarTitulosReporteProcesoCompleto(datos : datos, out datos : datos);
	
	foreach (objeto in this.form.listadoProcesos){
		FConfiguracion2.cargarContenidoReportesProcesoCompleto(proceso: objeto.objeto, datos: datos, out datos: datos);
	}
	
}else if(this.result == &amp;quot;djai&amp;quot;){
	FConfiguracion2.cargarTitulosReporteProcesoDJAI(datos : datos, out datos : datos);

	foreach (objeto in this.form.listadoProcesos){
		Respuesta respuesta2 = FListaDJAI2.getListValuesForIdProceso(procesoId : objeto.objeto.id);
		
		foreach(objetoDJAI in (NListaDJAI2[])respuesta2.contenido){ 
			FConfiguracion2.cargarContenidoReporteProcesoDJAI(djai : objetoDJAI, datos : datos, out datos : datos);
		}		
	}
	
}else if(this.result == &amp;quot;cta&amp;quot;){
	FConfiguracion2.cargarTitulosReporteProcesoCTA(datos : datos, out datos : datos);
	
	foreach (objeto in this.form.listadoProcesos){
		Respuesta respuesta2 = FListaCDV2.getListValuesForIdProceso(procesoId : objeto.objeto.id);
		
		foreach(objetoCTA in (NListaCDV2[])respuesta2.contenido){ 
			FConfiguracion2.cargarContenidoReporteProcesoCTA(cta : objetoCTA, datos : datos, out datos : datos);
		}		
	}
}

Excel.lineasDeTitulos = 1;
ByteArrayOutputStream xlsOutput = new ByteArrayOutputStream();
Excel.crearExcelBinario(buffer : xlsOutput, cantidadDeHojas : 1);
Excel.cargarDatos(datos : (Any[][])datos, nroHoja : 0, nombreHoja : &amp;quot;ListadoProcesos&amp;quot;);
Excel.grabar();

this.fileBinary = xlsOutput.toByteArray();
xlsOutput.close();

this.fileName = &amp;quot;ListadoProcesos_&amp;quot; + &amp;apos;now&amp;apos;.format(mask : &amp;quot;yyyyMMddHHmmss&amp;quot;)+&amp;quot;.xls&amp;quot;;&lt;/sourceCode&gt;
