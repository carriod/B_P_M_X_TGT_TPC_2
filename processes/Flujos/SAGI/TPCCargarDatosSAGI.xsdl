				this.configuracion.errores[&amp;quot;COMPRASPARTICULARES&amp;quot;][] = &amp;quot;Compras Particulares con faltantes o con errores.&amp;quot;;
			}
		}
		errores = {};
		errores = FDocumentacionAdjunta.verificarErrores(solicitud : this.solicitud, rol : this.configuracion.rol);
		
		if(errores.length() &amp;gt; 0){
			if(configuracion.proximaPantalla == &amp;quot;DOCUMENTACIONADJUNTA&amp;quot;){
				this.configuracion.errores[&amp;quot;DOCUMENTACIONADJUNTA&amp;quot;] = errores;
