			}
		}
	}
	else if(this.configuracion.actividad == &amp;quot;ACGAnalizarSAGI&amp;quot; ||
			this.configuracion.actividad == &amp;quot;ACGCompletarSAGI&amp;quot;||
			this.configuracion.actividad == &amp;quot;AnalizarSAGIPlanificada&amp;quot;){
		errores = {};
		errores = FControlGestion.verificarErrores(solicitud : this.solicitud, actividad : this.configuracion.actividad);
		if(errores.length() &amp;gt; 0){
			if(configuracion.proximaPantalla == &amp;quot;CONTROLGESTION&amp;quot;){
				this.configuracion.errores[&amp;quot;CONTROLGESTION&amp;quot;] = errores;
			}
			else{
				this.configuracion.errores[&amp;quot;CONTROLGESTION&amp;quot;][] = &amp;quot;Control Gestin con faltantes o con errores.&amp;quot;;
			}
		}
	}
	else if(this.configuracion.actividad == &amp;quot;CGCRecomendarSAGI&amp;quot; ||
			this.configuracion.actividad == &amp;quot;IGCRecomendarSAGI&amp;quot; ||
			this.configuracion.actividad == &amp;quot;OGCRecomendarSAGI&amp;quot;){
		errores = {};
		errores = FRecomendacionSAGI.verificarErrores(solicitud : this.solicitud);
		if(errores.length() &amp;gt; 0){
			if(configuracion.proximaPantalla == &amp;quot;RECOMENDARSAGI&amp;quot;){
				this.configuracion.errores[&amp;quot;RECOMENDARSAGI&amp;quot;] = errores;
			}
			else{
				this.configuracion.errores[&amp;quot;RECOMENDARSAGI&amp;quot;][] = &amp;quot;Recomendar SAGI con faltantes o con errores.&amp;quot;;
			}
		}
	} 
	else if((this.configuracion.actividad == &amp;quot;ACOResolucionPreComite&amp;quot; ||
	   		this.configuracion.actividad == &amp;quot;ACOResolucionComite&amp;quot;) &amp;amp;&amp;amp; 
	   		(this.configuracion.proximaPantalla != &amp;quot;RESOLUCION&amp;quot;)){
		errores = {};
		errores = FResolucion.verificarErrores(solicitud : this.solicitud);
		//if(errores.length() &amp;gt; 0){
			//if(configuracion.proximaPantalla == &amp;quot;RESOLUCION&amp;quot;){
			//	this.configuracion.errores[&amp;quot;RESOLUCION&amp;quot;] = errores;
			//}
			//else{
				this.configuracion.errores[this.configuracion.proximaPantalla][] = &amp;quot;Debe continuar desde la pantalla de Resolucin.&amp;quot;;
			//}
		//}
	}
}

if((this.configuracion.boton == &amp;quot;enviar&amp;quot; || this.configuracion.boton == &amp;quot;continuar&amp;quot;) &amp;amp;&amp;amp; this.configuracion.errores.length() == 0){
	this.configuracion.proximaPantalla = &amp;quot;&amp;quot;;
}
this.configuracion.cargaOK = false;
if(this.configuracion.boton == &amp;quot;verificar&amp;quot; &amp;amp;&amp;amp; (this.configuracion.errores == null || this.configuracion.errores.length() == 0)){
	this.configuracion.cargaOK = true;
