		&lt;method name=&quot;guardarSolicitud&quot;
			flags=&quot;public server&quot;&gt;
			&lt;sourceCode
				language=&quot;Java1&quot;&gt;Respuesta respuesta = Respuesta();

this.action = RELEASE;
if((this.configuracion.boton == &amp;quot;borrador&amp;quot; &amp;amp;&amp;amp; this.configuracion.actividad == &amp;quot;SSACompletarSAGI&amp;quot;) || 
   this.configuracion.boton == &amp;quot;guardar&amp;quot;){
	this.action = REPEAT;
}

if(this.configuracion.actividad == &amp;quot;CGCRecomendarSAGI&amp;quot; ||
	this.configuracion.actividad == &amp;quot;IGCRecomendarSAGI&amp;quot; ||
	this.configuracion.actividad == &amp;quot;OGCRecomendarSAGI&amp;quot; ||
	this.configuracion.actividad == &amp;quot;AnalizarSAGIPlanificada&amp;quot;){
	this.action = REPEAT;
}

//DEFINO EL ESTADO
Integer idEstado = 0;
if((this.configuracion.actividad == &amp;quot;SSASolicitarAltaSAGI&amp;quot; || 
   this.configuracion.actividad == &amp;quot;SSACompletarSAGI&amp;quot; ||
   this.configuracion.actividad == &amp;quot;GENCompletarSAGI&amp;quot;) &amp;amp;&amp;amp;
   this.configuracion.boton == &amp;quot;borrador&amp;quot;){
	idEstado = 1;
}
else if((this.configuracion.actividad == &amp;quot;SSASolicitarAltaSAGI&amp;quot; || 
   		this.configuracion.actividad == &amp;quot;SSACompletarSAGI&amp;quot;) &amp;amp;&amp;amp;
   		this.configuracion.boton == &amp;quot;enviar&amp;quot;){
   	this.solicitud.fechaEnvioEnlace = Time(&amp;apos;now&amp;apos;.format(&amp;quot;yyyyMMdd&amp;quot;));
	idEstado = 1;
}
else if(this.configuracion.actividad == &amp;quot;GENCompletarSAGI&amp;quot; &amp;amp;&amp;amp;
   		this.configuracion.boton == &amp;quot;enviar&amp;quot;){
	this.solicitud.fechaEnvioAnalisis = Time(&amp;apos;now&amp;apos;.format(&amp;quot;yyyyMMdd&amp;quot;));
	idEstado = 2;
}
else if(this.configuracion.actividad == &amp;quot;SCGAsignarParticipantesSAGI&amp;quot; &amp;amp;&amp;amp;
		this.configuracion.boton == &amp;quot;continuar&amp;quot;){
	idEstado = 3;
}
else if(this.configuracion.actividad == &amp;quot;ACGAnalizarSAGI&amp;quot; &amp;amp;&amp;amp;
		this.solicitud.controlGestion.fechaPlanificacionPreComite != null){
	idEstado = 4;
}
else if(this.configuracion.actividad == &amp;quot;ACOResolucionPreComite&amp;quot; ||
		this.configuracion.actividad == &amp;quot;ACOResolucionComite&amp;quot;){ 
	if(this.solicitud.resultadoResolucion != null){
		if(this.solicitud.resultadoResolucion == 6 ||
		   this.solicitud.resultadoResolucion == 8){
			idEstado = 5;
		}
		else if(this.solicitud.resultadoResolucion == 1 ||
				this.solicitud.resultadoResolucion == 7 ||
				this.solicitud.resultadoResolucion == 9 ||
				this.solicitud.resultadoResolucion == 10){
			idEstado = 7;
		}
		else{
			if(this.solicitud.fechaLiberacion == null){
				idEstado = 6;
			}
			else{
				idEstado = 7;
			}
		}
	}
}

if(this.solicitud.anulada){
	idEstado = 8;
}

this.solicitud.estado.idEstadoSolicitud = idEstado;
if(this.idSolicitud == null || this.idSolicitud == 0){
	this.solicitud.estado.usuarioAlta = this.solicitud.usuarioAlta;
}
else{
	this.solicitud.estado.usuarioModificacion = this.solicitud.usuarioModificacion;
}

//SETEO SI REQUIERE INTERVENCION Y SI REQUIERE COMPRAS
this.requiereIntervencion = false;
this.requiereCompras = false;
this.controllerAsignado = false;
this.compradorAsignado = false;
if(this.configuracion.actividad == &amp;quot;GENCompletarSAGI&amp;quot; &amp;amp;&amp;amp; configuracion.boton == &amp;quot;enviar&amp;quot;){
	if(this.solicitud.tipoCompra.id in {4,5}){
		this.requiereIntervencion = true;
	}
	else if(this.solicitud.tipoCompra.id in {1,2,3}){
		this.requiereCompras = true;
	}
}
//VERIFICO SI TIENE CONTROLER ASIGNADO
if(this.configuracion.actividad == &amp;quot;SCGAsignarParticipantesSAGI&amp;quot; &amp;amp;&amp;amp; this.configuracion.boton == &amp;quot;continuar&amp;quot;){
	if(this.solicitud.asignacionParticipante.controller != null &amp;amp;&amp;amp; this.solicitud.asignacionParticipante.controller != &amp;quot;&amp;quot;){
		this.controllerAsignado = true;
	}
}
//VERIFICO SI TIENE COMPRADOR ASIGNADO
if(this.configuracion.actividad == &amp;quot;GCOAsignarParticipantesSAGI&amp;quot; &amp;amp;&amp;amp; this.configuracion.boton == &amp;quot;continuar&amp;quot;){
	if(this.solicitud.asignacionParticipante.comprador != null &amp;amp;&amp;amp; this.solicitud.asignacionParticipante.comprador != &amp;quot;&amp;quot;){
		this.compradorAsignado = true;
	}
}

//GRABO LA SOLICITUD
if(this.solicitud.fechaAlta == null){
	this.solicitud.fechaAlta = Time(&amp;apos;now&amp;apos;.format(&amp;quot;yyyyMMdd&amp;quot;));
}else{
	this.solicitud.fechaAlta = Time(this.solicitud.fechaAlta.format(&amp;quot;yyyyMMdd&amp;quot;));
}
respuesta = FSolicitud.setValue(objNegocio : this.solicitud);

if(respuesta.resultado == OK) {
	this.solicitud = (NSolicitud)respuesta.contenido;
}
else{
	Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
	throw Java.Lang.Exception((String)respuesta.contenido);	
}

this.idSolicitud = this.solicitud.id;

if(idEstado != 0){
	//GRABO EL ESTADO DE LA SOLICITUD
	this.solicitud.estado.idSolicitud = this.solicitud.id;
	respuesta = FSolicitudEstado.setValue(objNegocio : this.solicitud.estado);
	if (respuesta.resultado == OK) {
		this.solicitud.estado = (NSolicitudEstado)respuesta.contenido;
	} else {
	    Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
		throw Java.Lang.Exception((String)respuesta.contenido);	
	}
}

//SETEO MENSAJE PARA LA SOLICITUD NUEVA
this.mensaje = &amp;quot;&amp;quot;;
if(this.configuracion.actividad == &amp;quot;SSASolicitarAltaSAGI&amp;quot; &amp;amp;&amp;amp; 
  (this.configuracion.boton == &amp;quot;borrador&amp;quot; || this.configuracion.boton == &amp;quot;enviar&amp;quot;)){
	this.mensaje = &amp;quot;Se gener correctamente la SAGI con Nro Solicitud &amp;quot; + this.idSolicitud + &amp;quot;.&amp;quot;;
}
else if(this.configuracion.actividad == &amp;quot;GENCompletarSAGI&amp;quot; &amp;amp;&amp;amp; this.solicitud.proveedor.tipoProveedor.id == 1 &amp;amp;&amp;amp; this.configuracion.boton == &amp;quot;enviar&amp;quot;){
	Java.Lang.Boolean doc3Cargado = false;
	ciclo:for (int index = 0; index &amp;lt; this.solicitud.documentos.length; index++) {
	    if(this.solicitud.documentos[index].documento.idTipoDocumento == 3 &amp;amp;&amp;amp; this.solicitud.documentos[index].documento.cargado){
	    	doc3Cargado = true;
	    	break ciclo;
	    }
	}
	if(!doc3Cargado){
		this.mensaje = &amp;quot;&amp;apos;Documento Carta Proveedor Condicionado&amp;apos; no fue cargado. Desea continuar?&amp;quot;;	
	}
}

//DEFINO PARTICIPANTE PARA EL ROL &amp;quot;TPC - Solicitante SAGI&amp;quot;
if((this.configuracion.actividad == &amp;quot;SSASolicitarAltaSAGI&amp;quot;) &amp;amp;&amp;amp; 
  (this.solicitud.gestorSolicitante != null &amp;amp;&amp;amp; this.solicitud.gestorSolicitante != &amp;quot;&amp;quot;)){
	this.participantSolicitante = Participant.find(name : this.solicitud.gestorSolicitante);
}
//DEFINO PARTICIPANTE PARA EL ROL &amp;quot;TPC - Gestor Enlace&amp;quot;
if((this.configuracion.actividad == &amp;quot;SSASolicitarAltaSAGI&amp;quot; || 
   this.configuracion.actividad == &amp;quot;SSACompletarSAGI&amp;quot; ||
   this.configuracion.actividad == &amp;quot;GENCompletarSAGI&amp;quot;) &amp;amp;&amp;amp; 
   (this.solicitud.gestorEnlace != null &amp;amp;&amp;amp; this.solicitud.gestorEnlace != &amp;quot;&amp;quot;)){
	this.participantGestor = Participant.find(name : this.solicitud.gestorEnlace);
}
//DEFINO PARTICIPANTE PARA EL ROL &amp;quot;TPC - Analista CdG&amp;quot;
if((this.configuracion.actividad == &amp;quot;SCGAsignarParticipantesSAGI&amp;quot;) &amp;amp;&amp;amp; 
   (this.solicitud.asignacionParticipante.analista != null &amp;amp;&amp;amp; this.solicitud.asignacionParticipante.analista != &amp;quot;&amp;quot;)){
	this.participantAnalista = Participant.find(name : this.solicitud.asignacionParticipante.analista);
	if(this.solicitud.asignacionParticipante.controller == null || this.solicitud.asignacionParticipante.controller == &amp;quot;&amp;quot;){
		this.participantController = Participant.find(name : this.solicitud.asignacionParticipante.analista);
	}
}

if((this.configuracion.actividad == &amp;quot;ACOResolucionPreComite&amp;quot; ||
	this.configuracion.actividad == &amp;quot;ACOResolucionComite&amp;quot;) &amp;amp;&amp;amp; 
   (this.solicitud.asignacionParticipante.analista != null &amp;amp;&amp;amp; this.solicitud.asignacionParticipante.analista != &amp;quot;&amp;quot;)){
	this.participantAnalista = Participant.find(name : this.solicitud.asignacionParticipante.analista);
}
//DEFINO PARTICIPANTE PARA EL ROL &amp;quot;TPC - Controller GC&amp;quot;
if((this.configuracion.actividad == &amp;quot;SCGAsignarParticipantesSAGI&amp;quot; ||
   this.configuracion.actividad == &amp;quot;ACGAsignarParticipantesSAGI&amp;quot;) &amp;amp;&amp;amp; 
   (this.solicitud.asignacionParticipante.controller != null &amp;amp;&amp;amp; this.solicitud.asignacionParticipante.controller != &amp;quot;&amp;quot;)){
	this.participantController = Participant.find(name : this.solicitud.asignacionParticipante.controller);
}
//DEFINO PARTICIPANTE PARA EL ROL &amp;quot;TPC - Jefe Compras&amp;quot;
if((this.configuracion.actividad == &amp;quot;GCOAsignarParticipantesSAGI&amp;quot;) &amp;amp;&amp;amp; 
   (this.solicitud.asignacionParticipante.jefeCompras != null &amp;amp;&amp;amp; this.solicitud.asignacionParticipante.jefeCompras != &amp;quot;&amp;quot;)){
	this.participantJefeCompras = Participant.find(name : this.solicitud.asignacionParticipante.jefeCompras);
}
//DEFINO PARTICIPANTE PARA EL ROL &amp;quot;TPC - Comprador GC&amp;quot;
if((this.configuracion.actividad == &amp;quot;GCOAsignarParticipantesSAGI&amp;quot; ||
   this.configuracion.actividad == &amp;quot;JCOAsignarParticipantesSAGI&amp;quot;) &amp;amp;&amp;amp; 
   (this.solicitud.asignacionParticipante.comprador != null &amp;amp;&amp;amp; this.solicitud.asignacionParticipante.comprador != &amp;quot;&amp;quot;)){
	this.participantComprador = Participant.find(name : this.solicitud.asignacionParticipante.comprador);
}

if(this.configuracion.actividad == &amp;quot;ACOResolucionPreComite&amp;quot; ||
	this.configuracion.actividad == &amp;quot;ACOResolucionComite&amp;quot;){
	if(this.solicitud.resultadoResolucion != null &amp;amp;&amp;amp; this.solicitud.resultadoResolucion != 0){
		respuesta = FResultadoResolucion.getValueForId(id : this.solicitud.resultadoResolucion);
		if (respuesta.resultado == OK) {
			this.resultadoResolucionDesc = ((NResultadoResolucion)respuesta.contenido).descripcion;
		}else{
			Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
			throw Java.Lang.Exception((String)respuesta.contenido);
		}
	}
}

NRecomendacionSAGI reco = NRecomendacionSAGI();

this.recomendacionAnalista = &amp;quot;&amp;quot;;
respuesta = FRecomendacionSAGI.getLastValueForRol(rol : &amp;quot;TPC - Analista CdG&amp;quot;, idSol : this.solicitud.id);
if(respuesta.resultado == OK){
    reco = (NRecomendacionSAGI)respuesta.contenido;
    this.recomendacionAnalista = reco.recomendacion.substring(first : 0, last : 254);
}else if(respuesta.resultado == DATAERROR){
}else{
    Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
    throw Java.Lang.Exception((String)respuesta.contenido);
}

respuesta = FRecomendacionSAGI.getLastValueForRol(rol : &amp;quot;TPC - Controller GC&amp;quot;, idSol : this.solicitud.id);
if(respuesta.resultado == OK){
    reco = (NRecomendacionSAGI)respuesta.contenido;
    this.recomendacionController = reco.recomendacion.substring(first : 0, last : 254);
}else if(respuesta.resultado == DATAERROR){
}else{
    Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
    throw Java.Lang.Exception((String)respuesta.contenido);
}

this.recomendacionCompras = &amp;quot;&amp;quot;;
respuesta = FRecomendacionSAGI.getLastValueForRol(rol : &amp;quot;TPC - Comprador GC&amp;quot;, idSol : this.solicitud.id);
if(respuesta.resultado == OK){
    reco = (NRecomendacionSAGI)respuesta.contenido;
    this.recomendacionCompras = reco.recomendacion.substring(first : 0, last : 254);
}else if(respuesta.resultado == DATAERROR){
}else{
    Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
    throw Java.Lang.Exception((String)respuesta.contenido);
}

this.recomendacionIntervencion = &amp;quot;&amp;quot;;
respuesta = FRecomendacionSAGI.getLastValueForRol(rol : &amp;quot;TPC - Intervencion GC&amp;quot;, idSol :  this.solicitud.id);
if(respuesta.resultado == OK){
    reco = (NRecomendacionSAGI)respuesta.contenido;
    this.recomendacionIntervencion = reco.recomendacion.substring(first : 0, last : 254);
}else if(respuesta.resultado == DATAERROR){
}else{
    Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
    throw Java.Lang.Exception((String)respuesta.contenido);
}


respuesta = FResolucion.getLastValueForIdSol(idSol : this.solicitud.id);
if(respuesta.resultado == OK){
    ultimaResolucion = (NResolucion)respuesta.contenido;
}else if(respuesta.resultado == DATAERROR){
}else{
    Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
    throw Java.Lang.Exception((String)respuesta.contenido);
}

this.sociedades = &amp;quot;&amp;quot;;
for(int i = 0; i &amp;lt; this.solicitud.sociedades.length(); i++){
    this.sociedades = this.sociedades + this.solicitud.sociedades[i].sociedad.descripcion + &amp;quot; &amp;quot;;
}
this.sociedades = this.sociedades.substring(first : 0, last : 254);

this.nombreProyectoMayor = &amp;quot;&amp;quot;;
for(int i = 0; i &amp;lt; this.solicitud.proyectosMayores.length(); i++){
    this.nombreProyectoMayor = this.nombreProyectoMayor + this.solicitud.proyectosMayores[i].proyectoMayor.nombreProyecto.descripcion + &amp;quot; - &amp;quot; + this.solicitud.proyectosMayores[i].proyectoMayor.porcentajeIncidencia + &amp;quot;% &amp;quot;;
}
this.nombreProyectoMayor = this.nombreProyectoMayor.substring(first : 0, last : 254);

//GENERO LA INSTANCIA PARA LA SOLICITUD EN CASO DE SER DUPLICADA
if(this.duplicada){ 
	if(this.solicitud.gestorSolicitante != null &amp;amp;&amp;amp; this.solicitud.gestorSolicitante != &amp;quot;&amp;quot;){
		this.participantSolicitante = Participant.find(name : this.solicitud.gestorSolicitante);
	}
	if(this.solicitud.gestorEnlace != null &amp;amp;&amp;amp; this.solicitud.gestorEnlace != &amp;quot;&amp;quot;){
		this.participantGestor = Participant.find(name : this.solicitud.gestorEnlace);
	}
	ProcessInstance.create(processId : &amp;quot;/ProcesoDeTableroDePedidosYCompras&amp;quot;, 
           arguments : {	&amp;quot;botonArg&amp;quot; : this.configuracion.boton,
							&amp;quot;actionArg&amp;quot; : this.action,
