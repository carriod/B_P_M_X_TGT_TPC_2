			&lt;sourceCode
				language=&quot;Java1&quot;&gt;this.configuracion.errores.clear();

String[] errores = {};
if(configuracion.boton == &amp;quot;enviar&amp;quot; || configuracion.boton == &amp;quot;verificar&amp;quot; || configuracion.boton == &amp;quot;continuar&amp;quot;){
	if(this.configuracion.actividad == &amp;quot;SSASolicitarAltaSAGI&amp;quot; || 
   	   this.configuracion.actividad == &amp;quot;SSACompletarSAGI&amp;quot; ||
  	   this.configuracion.actividad == &amp;quot;GENCompletarSAGI&amp;quot;){	
  	   	
		errores = {};
		errores = FSolicitud.verificarErrores(solicitud : this.solicitud, rol : this.configuracion.rol);
		if(errores.length() &amp;gt; 0){
			if(configuracion.proximaPantalla == &amp;quot;DATOSGENERALES&amp;quot;){
				this.configuracion.errores[&amp;quot;DATOSGENERALES&amp;quot;] = errores;
			}
			else{ 
				this.configuracion.errores[&amp;quot;DATOSGENERALES&amp;quot;][] = &amp;quot;Datos Generales con faltantes o con errores.&amp;quot;;
			}
		}
		errores = {};
		errores = FValoracionNecesidad.verificarErrores(solicitud : this.solicitud, rol: this.configuracion.rol);
		if(errores.length() &amp;gt; 0){
			if(configuracion.proximaPantalla == &amp;quot;VALORACIONNECESIDAD&amp;quot;){
				this.configuracion.errores[&amp;quot;VALORACIONNECESIDAD&amp;quot;] = errores;
			}
			else{
				this.configuracion.errores[&amp;quot;VALORACIONNECESIDAD&amp;quot;][] = &amp;quot;Valoracin de la Necesidad con faltantes o con errores.&amp;quot;;
			}
		}
		errores = {};
		errores = FProyectosAsociados.verificarErrores(solicitud : this.solicitud, rol: this.configuracion.rol);
		if(errores.length() &amp;gt; 0){
			if(configuracion.proximaPantalla == &amp;quot;PROYECTOSASOCIADOS&amp;quot;){
				this.configuracion.errores[&amp;quot;PROYECTOSASOCIADOS&amp;quot;] = errores;
			}
			else{
				this.configuracion.errores[&amp;quot;PROYECTOSASOCIADOS&amp;quot;][] = &amp;quot;Proyectos Asociados (Inversin o Gasto) con faltantes o con errores.&amp;quot;;
			}
		}
		errores = {};
		errores = FProveedor.verificarErrores(solicitud : this.solicitud, rol: this.configuracion.rol);
		if(errores.length() &amp;gt; 0){
			if(configuracion.proximaPantalla == &amp;quot;PROVEEDOR&amp;quot;){
				this.configuracion.errores[&amp;quot;PROVEEDOR&amp;quot;] = errores;
			}
			else{
				this.configuracion.errores[&amp;quot;PROVEEDOR&amp;quot;][] = &amp;quot;Proveedor con faltantes o con errores.&amp;quot;;
			}
		}
		errores = {};
		errores = FAntecedentes.verificarErrores(solicitud : this.solicitud, rol : this.configuracion.rol);
		if(errores.length() &amp;gt; 0){
			if(configuracion.proximaPantalla == &amp;quot;ANTECEDENTES&amp;quot;){
				this.configuracion.errores[&amp;quot;ANTECEDENTES&amp;quot;] = errores;
			}
			else{
				this.configuracion.errores[&amp;quot;ANTECEDENTES&amp;quot;][] = &amp;quot;Antecedentes con faltantes o con errores.&amp;quot;;
			}
		}
		errores = {};
		errores = FSituacionPresupuestaria.verificarErrores(solicitud : this.solicitud, rol : this.configuracion.rol);
		if(errores.length() &amp;gt; 0){
			if(configuracion.proximaPantalla == &amp;quot;SITUACIONPRESUPUESTARIA&amp;quot;){
				this.configuracion.errores[&amp;quot;SITUACIONPRESUPUESTARIA&amp;quot;] = errores;
			}
			else{
				this.configuracion.errores[&amp;quot;SITUACIONPRESUPUESTARIA&amp;quot;][] = &amp;quot;Situacin Presupuestaria con faltantes o con errores.&amp;quot;;
			}
		}
		errores = {};
		errores = FComprasParticulares.verificarErrores(solicitud : this.solicitud, rol : this.configuracion.rol);
		if(errores.length() &amp;gt; 0){
			if(configuracion.proximaPantalla == &amp;quot;COMPRASPARTICULARES&amp;quot;){
				this.configuracion.errores[&amp;quot;COMPRASPARTICULARES&amp;quot;] = errores;
			}
