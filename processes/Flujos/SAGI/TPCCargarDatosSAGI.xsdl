				this.configuracion.errores[&amp;quot;DOCUMENTACIONADJUNTA&amp;quot;][] = &amp;quot;Documentacin Adjunta con faltantes o con errores.&amp;quot;;
			}	
		}
	}
	else if(this.configuracion.actividad == &amp;quot;SCGAsignarParticipantesSAGI&amp;quot; ||
	   		this.configuracion.actividad == &amp;quot;ACGAsignarParticipantesSAGI&amp;quot; ||
		    this.configuracion.actividad == &amp;quot;GCOAsignarParticipantesSAGI&amp;quot; ||
		    this.configuracion.actividad == &amp;quot;JCOAsignarParticipantesSAGI&amp;quot;){	
		errores = {};
		errores = FAsignacionParticipante.verificarErrores(solicitud : this.solicitud, rol : this.configuracion.rol, actividad : this.configuracion.actividad);
		if(errores.length() &amp;gt; 0){
			if(configuracion.proximaPantalla == &amp;quot;ASIGNARPARTICIPANTE&amp;quot;){
				this.configuracion.errores[&amp;quot;ASIGNARPARTICIPANTE&amp;quot;] = errores;
