			&lt;sourceCode
				language=&quot;Java1&quot;&gt;if(this.documentoCargado){
	NSolicitudDocumentos solicitudDocumento = NSolicitudDocumentos();
	solicitudDocumento.idSolicitud = this.solicitud.id;
	solicitudDocumento.idTipoDocumento = this.idTipoDocumento;
	solicitudDocumento.nombreArchivo = this.nombreArchivo;
	solicitudDocumento.extensionArchivo = this.extensionArchivo;
	solicitudDocumento.archivoBinario = this.archivoBinario;
	solicitudDocumento.usuarioAlta = Participant.id;
	solicitudDocumento.usuarioModificacion = Participant.id;
	solicitudDocumento.cargado = true;
	Java.Lang.Boolean existe = false;
	Integer indiceDoc = 0;
	for (int index = 0; index &amp;lt; this.form.solicitud.documentos.length; index++) {
	    if(this.form.solicitud.documentos[index].documento.idTipoDocumento == this.idTipoDocumento){
	    	existe = true;
	    	indiceDoc = index;
	    }
	}
	if(existe){		
		this.form.solicitud.documentos[indiceDoc].documento = solicitudDocumento;
		if(this.solicitud.id != null &amp;amp;&amp;amp; this.solicitud.id != 0){
			Respuesta respuesta = FSolicitudDocumentos.setValue(objNegocio : solicitudDocumento);
			if(respuesta.resultado != OK){
				Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
				throw Java.Lang.Exception((String)respuesta.contenido);
			}
		}
	}
	else{
		this.form.solicitud.documentos.extend(documento : solicitudDocumento);
	}
