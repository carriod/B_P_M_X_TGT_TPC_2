				language=&quot;Java1&quot;&gt;Utiles.Respuesta respuesta = Utiles.Respuesta();

for(int i = 0; i &amp;lt; form.busqueda.length(); i++){
	if(form.busqueda[i].analista != null){
		respuesta = FAsignacionParticipante.setAnalistaForId(idSolicitud : form.busqueda[i].resultado.idSolicitud,
																analista : form.busqueda[i].analista);
		if (respuesta.resultado != OK) {
			throw Java.Lang.Exception((String)respuesta.contenido);	
		}
		//GRABO EL ESTADO DE LA SOLICITUD
		this.solicitud.estado.idSolicitud = form.busqueda[i].resultado.idSolicitud;
		this.solicitud.estado.idEstadoSolicitud = 3;
		
		respuesta = FSolicitudEstado.setValue(objNegocio : this.solicitud.estado);
		if (respuesta.resultado == OK) {
			this.solicitud.estado = (NSolicitudEstado)respuesta.contenido;
		} else {
			throw Java.Lang.Exception((String)respuesta.contenido);	
		}
	}
}



