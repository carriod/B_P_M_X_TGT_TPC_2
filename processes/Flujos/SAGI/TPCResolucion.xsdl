				language=&quot;Java1&quot;&gt;this.form.botonesAccion.clear();
this.form.botonesSolapa.clear();

this.form.botonesAccion.extend(labelBoton : &amp;quot;Volver&amp;quot;);

String[] botonVer = this.result.split(delim : &amp;quot;_&amp;quot;);
Long idRes = (Long)botonVer[1];

Respuesta respuesta = FResolucion.getValueForId(id : idRes);
NResolucion res;

if (respuesta.resultado == OK){
	res = (NResolucion)respuesta.contenido;
	this.form.resolucionDetalle = res;
	this.form.participantesDetalle.clear();
	for (int i = 0; i&amp;lt;res.participantes.length();i++){
		this.form.participantesDetalle.extend(participante : res.participantes[i].participante);
	}

}&lt;/sourceCode&gt;
			&lt;property name=&quot;RelayToSupported&quot;
				value=&quot;true&quot;/&gt;
		&lt;/method&gt;
		&lt;method name=&quot;salida&quot;
			flags=&quot;public server&quot;&gt;
			&lt;sourceCode
				language=&quot;Java1&quot;&gt;FConfiguracion.setSalidaPantalla(configuracion : this.configuracion, result: result, out configuracion : this.configuracion);

this.form.resolucion.participantes.clear();
for (int i = 0; i&amp;lt;this.form.participantesResolucion.length();i++){
	this.form.resolucion.participantes.extend(participante : this.form.participantesResolucion[i].participante);
}

this.solicitud.resultadoResolucion = this.form.resolucion.resultadoResolucion.id;
this.solicitud.fechaLiberacion = this.form.resolucion.fechaLiberacion;
if(this.form.resolucion.comentario != this.ultimoComentarioResolucion){
	this.solicitud.comentarioResolucion = this.form.resolucion.comentario;
}
this.solicitud.comentario.fecha = &amp;apos;now&amp;apos;;
this.solicitud.comentario.usuario = Participant.id;
if(this.configuracion.actividad == &amp;quot;ACOResolucionPreComite&amp;quot;){
	this.solicitud.fechaPlanPreComite = this.form.resolucion.fechaPlanificada;
	this.solicitud.fechaRealPreComite = this.form.resolucion.fechaReal;
}
else if(this.configuracion.actividad ==  &amp;quot;ACOResolucionComite&amp;quot;){
	this.solicitud.fechaPlanComite = this.form.resolucion.fechaPlanificada;
	this.solicitud.fechaRealComite = this.form.resolucion.fechaReal;
}
this.form.solicitud.resoluciones.clear();
for (int index = 0; index &amp;lt; this.form.resoluciones.length; index++) {
    this.form.solicitud.resoluciones.extend(resolucion : this.form.resoluciones[index].resolucion);
}
if(this.configuracion.boton != &amp;quot;continuar&amp;quot; &amp;amp;&amp;amp; this.configuracion.boton != &amp;quot;guardar&amp;quot; &amp;amp;&amp;amp; this.configuracion.boton != &amp;quot;cancelar&amp;quot;){
	this.form.resolucion.borrador = true;
	if(this.form.resolucion.resultadoResolucion.id == 8){
		this.solicitud.fechaPlanPreComite = this.form.resolucion.fechaProximoPreComite;
		this.solicitud.fechaRealPreComite = this.form.resolucion.fechaProximoPreComite;
		this.form.resolucion.tipo = &amp;quot;P&amp;quot;;
	}
	else if(this.form.resolucion.resultadoResolucion.id == 6){
		this.solicitud.fechaPlanComite = this.form.resolucion.fechaProximoComite;
		this.solicitud.fechaRealComite = this.form.resolucion.fechaProximoComite;
		this.form.resolucion.tipo = &amp;quot;C&amp;quot;;
	}
	this.form.solicitud.resoluciones.extend(resolucion : this.form.resolucion);
}

if(this.configuracion.boton == &amp;quot;continuar&amp;quot;){
	NResolucion res = NResolucion();
	res.borrador = true;
