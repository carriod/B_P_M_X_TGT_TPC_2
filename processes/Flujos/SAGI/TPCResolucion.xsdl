				participant = Participant.find(name : asiPar.comprador);
				resPar.rol = &amp;quot;TPC - Comprador GC&amp;quot;;
				resPar.email = participant.email;
				this.form.participantesResolucion.extend(participante : resPar);
			}
			if (this.solicitud.requiereIntervencion){
				resPar = NResolucionParticipantes();
				resPar.participante = asiPar.intervencion;
				participant = Participant.find(name : asiPar.intervencion);
				resPar.rol = &amp;quot;TPC - Intervencion GC&amp;quot;;
				resPar.email = participant.email;
				this.form.participantesResolucion.extend(participante : resPar);
			}
			
			if (this.configuracion.actividad == &amp;quot;ACOResolucionComite&amp;quot;){
				resPar = NResolucionParticipantes();
				resPar.participante = this.solicitud.gestorSolicitante;
				resPar.rol = &amp;quot;TPC - Solicitante SAGI&amp;quot;;
				resPar.email = this.solicitud.emailSolicitante;
				this.form.participantesResolucion.extend(participante : resPar);
				
				resPar = NResolucionParticipantes();
