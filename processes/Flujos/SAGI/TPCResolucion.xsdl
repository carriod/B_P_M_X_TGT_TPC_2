	//this.form.resolucion.fechaPlanificada = this.form.solicitud.controlGestion.fechaPlanificacionPreComite;
	//this.form.resolucion.fechaReal = this.form.resolucion.fechaPlanificada;

	for (int i = 0;i&amp;lt;this.solicitud.resoluciones.length();i++){
		if (this.solicitud.resoluciones[i].resolucion.borrador){
			this.form.resolucion = this.solicitud.resoluciones[i].resolucion;
			this.ultimoComentarioResolucion = this.solicitud.resoluciones[i].resolucion.comentario;
		}
		else{
			this.form.resoluciones.extend(resolucion : this.solicitud.resoluciones[i].resolucion);
		}
	}
		if(this.form.actividad == &amp;quot;ACOResolucionPreComite&amp;quot;){
		this.form.resolucion.tipo = &amp;quot;P&amp;quot;;
		this.form.resolucion.fechaPlanificada = this.form.solicitud.fechaPlanPreComite;
		if(this.form.solicitud.fechaRealPreComite == null){
			this.form.resolucion.fechaReal = this.form.resolucion.fechaPlanificada;
		}
		else{
			this.form.resolucion.fechaReal = this.form.solicitud.fechaRealPreComite;
		}
	}
	else if(this.form.actividad == &amp;quot;ACOResolucionComite&amp;quot;){
		this.form.resolucion.tipo = &amp;quot;C&amp;quot;;
		this.form.resolucion.fechaPlanificada = this.form.solicitud.fechaPlanComite;
		if(this.form.solicitud.fechaRealComite == null){
			this.form.resolucion.fechaReal = this.form.resolucion.fechaPlanificada;
		}
		else{
			this.form.resolucion.fechaReal = this.form.solicitud.fechaRealComite;
		}
	}
	//if(!this.form.resolucion.borrador){
		this.form.montoAprobadoPesos 		= this.solicitud.montoTotalPesos;
		this.form.montoAprobadoDolares 		= this.solicitud.montoTotalDolares;
		this.form.montoAprobadoEuros 		= this.solicitud.montoTotalEuros;
		this.form.montoAprobadoOtro 		= this.solicitud.montoTotalOtro;
		this.form.montoAprobadoOtrosId 		= this.solicitud.montoTotalOM.id;
	//}
	this.form.resolucion.borrador = false;
	if (this.form.resolucion.participantes.length() == 0){
		Respuesta respuesta;
		NAsignacionParticipante asiPar;
		respuesta = FAsignacionParticipante.getValueForId(idSol : this.solicitud.id);
		if (respuesta.resultado == OK){
			asiPar = (NAsignacionParticipante) respuesta.contenido;
			
			NResolucionParticipantes resPar = NResolucionParticipantes();
			resPar.participante = asiPar.analista;
			Participant participant = Participant.find(name : asiPar.analista);
			resPar.rol = &amp;quot;TPC - Analista CdG&amp;quot;;
			resPar.email = participant.email;
			this.form.participantesResolucion.extend(participante : resPar);
			/*
			resPar = NResolucionParticipantes();
