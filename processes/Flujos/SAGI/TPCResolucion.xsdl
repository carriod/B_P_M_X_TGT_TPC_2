}

//EDITABLE
this.form.editable = this.configuracion.pantallaEditable[&amp;quot;RESOLUCION&amp;quot;];

//DATOS DE PANTALLA
if (!this.form.vuelveDetalle){
	this.form.resoluciones.clear();
