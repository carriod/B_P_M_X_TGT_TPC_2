				language=&quot;Java1&quot;&gt;Utiles.Logger.logDebug(mensaje : &amp;quot;botonInput: &amp;quot; + this.botonInput);

this.form = Formulario.FormArchivos();
String [] btn = this.botonInput.split(delim : &amp;quot;_&amp;quot;);
this.botonInput = btn[0];
this.idSolicitud = (int)btn[1];
int idTipoDocumento = (int)btn[2];
int version = 1;
Respuesta respuesta = Respuesta();
if(idSolicitud != null &amp;amp;&amp;amp; idSolicitud != 0){
	respuesta = FEasyDoc.getToken(nroReg : idSolicitud, 
								  idDoc : idTipoDocumento, 	
								  version : version, 
								  usuario : Participant.id);
	if (respuesta.resultado == OK) {
		this.token = String(respuesta.contenido);
		Utiles.Logger.logDebug(mensaje : &amp;quot;token: &amp;quot; + token);
	}
	else{
		Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
		throw Java.Lang.Exception((String)respuesta.contenido);
	}

	if (this.botonInput == &amp;quot;ver&amp;quot;) {
	    respuesta = FEasyDoc.getFile(token : this.token);
	    if (respuesta.resultado == OK) {
			this.fileBinary = (Binary) respuesta.contenido;
		}
		else{
			Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
			throw Java.Lang.Exception((String)respuesta.contenido);
		}
	    this.fileDescripcion = &amp;quot;Archivo Adjunto&amp;quot;;
	    this.fileName = &amp;quot;ArchivoAdjunto_&amp;quot; + &amp;apos;now&amp;apos;.format(mask : &amp;quot;yyyyMMddhhmmsss&amp;quot;) + &amp;quot;.&amp;quot; + this.fileExtension;
	    this.fileLength = this.fileBinary.length();
	} 
	else if(this.botonInput == &amp;quot;cargar&amp;quot;){
	
	} 
	else if (this.botonInput == &amp;quot;borrar&amp;quot;){
	    Respuesta respuesta2 = FEasyDoc.deleteFile(token : this.token);
