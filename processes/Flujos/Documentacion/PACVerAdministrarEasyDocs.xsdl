			&lt;sourceCode
				language=&quot;Java1&quot;&gt;Respuesta respuesta;

if(this.form == null){
	this.form = FormVerAdmEasyDocs();
}  
this.form.titulo = &amp;quot;Administrar Documento: &amp;quot; + this.tituloDocumento;
this.form.botones.clear();
this.form.botones.extend(labelBoton : &amp;quot;Volver&amp;quot;);
this.form.botones.extend(labelBoton : null);
this.form.botones.extend(labelBoton : null);
this.form.botones.extend(labelBoton : null);
this.form.botones.extend(labelBoton : null);
this.form.botones.extend(labelBoton : &amp;quot;Generar ZIP&amp;quot;);
this.form.botones.extend(labelBoton : &amp;quot;Generar PDF&amp;quot;);
this.form.botones.extend(labelBoton : &amp;quot;Borrar&amp;quot;);
this.form.permiteBorrar = this.permiteBorrar;
this.form.documentos.clear();
try{
	respuesta = FEasyDoc.listFiles(token : this.token);
	if (respuesta.resultado == OK) {

	Any[]archivos = Any[](respuesta.contenido);

		for(int i = 0;i &amp;lt; archivos.length();i ++){
			Any[String] fila = (Any[String])archivos[i]; 
			this.form.documentos[i].nombreUsuario = (String)fila[&amp;quot;UsuarioCarga&amp;quot;];
			this.form.documentos[i].nombreArchivo = (String)fila[&amp;quot;NombreArchivo&amp;quot;];
			this.form.documentos[i].extension = ((String) fila[&amp;quot;Extension&amp;quot;]).substring(first : 1, last : ((String) fila[&amp;quot;Extension&amp;quot;]).length());
			this.form.documentos[i].codigoLote = (String)fila[&amp;quot;CodigoLote&amp;quot;];
			this.form.documentos[i].fechaCarga = (Time)fila[&amp;quot;FechaCarga&amp;quot;];
		}
	}
}
catch (Java.Lang.Exception e) {
    respuesta.resultado = DBERROR;
    respuesta.contenido = &amp;quot;fachada.listFiles(): Error &amp;quot; + e.message;
    Utiles.Logger.logException(exception : e);
}&lt;/sourceCode&gt;
