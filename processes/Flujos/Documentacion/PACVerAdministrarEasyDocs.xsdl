			&lt;sourceCode
				language=&quot;Java1&quot;&gt;Respuesta respuesta;

if(this.botonInput.indexOf(text : &amp;quot;ver&amp;quot;)==0){

		String [] btn = this.botonInput.split(delim : &amp;quot;*&amp;quot;);
		this.botonInput = btn[0];
		String codigoLote = (String)btn[1];
	
		respuesta = FEasyDoc.getIndividualFile(token : this.token, codigoLote : codigoLote);
		if (respuesta.resultado == OK) {
			this.fileBinary = (Binary) respuesta.contenido;
		}
		else{
			Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
			throw Java.Lang.Exception((String)respuesta.contenido);
		}
		this.fileExtension = &amp;quot;pdf&amp;quot;;
		for(int i = 0;i &amp;lt; this.form.documentos.length();i ++){
			if(this.form.documentos[i].codigoLote.trim() == codigoLote.trim()){
				this.fileExtension = this.form.documentos[i].extension;
			}
		}

	    this.fileDescripcion = &amp;quot;Archivo Adjunto&amp;quot;;
	    this.fileName = &amp;quot;ArchivoAdjunto_&amp;quot; + &amp;apos;now&amp;apos;.format(mask : &amp;quot;yyyyMMddhhmmsss&amp;quot;) + &amp;quot;.&amp;quot; + this.fileExtension;
	    this.fileLength = this.fileBinary.length();
		
} else if(this.botonInput == &amp;quot;generar pdf&amp;quot;){
	String[] codigosLote = { };

	for(int i=0;i &amp;lt; this.form.documentos.length();i ++){
		if(this.form.documentos[i].check == true){
			codigosLote[] = this.form.documentos[i].codigoLote;
		}
	}
	
	respuesta = FEasyDoc.generatePDF(token : this.token, codigosLote : codigosLote);
	if (respuesta.resultado == OK) {
			this.fileBinary = (Binary) respuesta.contenido;
	}
	else{
		Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
		throw Java.Lang.Exception((String)respuesta.contenido);
	}	
	
	this.fileExtension = &amp;quot;pdf&amp;quot;;
	this.fileDescripcion = &amp;quot;Archivo Adjunto&amp;quot;;
	this.fileName = &amp;quot;ArchivoAdjunto_&amp;quot; + &amp;apos;now&amp;apos;.format(mask : &amp;quot;yyyyMMddhhmmsss&amp;quot;) + &amp;quot;.pdf&amp;quot;;
	this.fileLength = this.fileBinary.length();

} else if(this.botonInput == &amp;quot;generar zip&amp;quot;){
	String[] codigosLote = { };

	for(int i=0;i &amp;lt; this.form.documentos.length();i ++){
		if(this.form.documentos[i].check == true){
			codigosLote[] = this.form.documentos[i].codigoLote;
		}
	}
	
	respuesta = FEasyDoc.generateZIP(token : this.token, codigosLote : codigosLote);
	if (respuesta.resultado == OK) {
			this.fileBinary = (Binary) respuesta.contenido;
	}
	else{
		Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
		throw Java.Lang.Exception((String)respuesta.contenido);
	}	
	
	this.fileExtension = &amp;quot;zip&amp;quot;;
	this.fileDescripcion = &amp;quot;Archivo Adjunto&amp;quot;;
	this.fileName = &amp;quot;ArchivoAdjunto_&amp;quot; + &amp;apos;now&amp;apos;.format(mask : &amp;quot;yyyyMMddhhmmsss&amp;quot;) + &amp;quot;.zip&amp;quot;;
	this.fileLength = this.fileBinary.length();

} else if(this.botonInput == &amp;quot;borrar&amp;quot;){
	String codigoLote = &amp;quot;&amp;quot;;
	for(int i=0;i &amp;lt; this.form.documentos.length();i ++){
		if(this.form.documentos[i].check == true){
			codigoLote = this.form.documentos[i].codigoLote;
			respuesta = FEasyDoc.deleteIndividualFile(token : this.token, codigoLote : codigoLote);
			if (respuesta.resultado != OK) {
				Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
				throw Java.Lang.Exception((String)respuesta.contenido);
			}
		}
	}		
