			&lt;sourceCode
				language=&quot;Java1&quot;&gt;Respuesta respuesta;
Any[][] datos = {};

if(result == &amp;quot;cartas&amp;quot;){
	FConfiguracion2.cargarTitulosReporteCartaCompleto(datos : datos, out datos : datos);
	Integer[] solicitudes = {};

	foreach (objeto in this.form.listaPedidos){
		respuesta = FCarta2.getValuesForPedido(proId : objeto.objeto.procesoId,
	    	                                   proEdicion : objeto.objeto.procesoEdicion,
	        	                               prvCodigo : objeto.objeto.proveedor.codigo, 
	            	                           monId : objeto.objeto.moneda.id,
	                	                       socId : objeto.objeto.sociedad.id,
	                	                       formaDePagoCodigo : objeto.objeto.formaDePago.codigo);
	                	                       
		foreach(carta in (NCarta2[]) respuesta.contenido){
			FConfiguracion2.cargarContenidoReportesCartaCompleto(datos : datos, carta : carta, 
	        		                                             moneda : objeto.objeto.moneda, sociedad : objeto.objeto.sociedad, 
	                		                                     proveedor : objeto.objeto.proveedor, out datos : datos);
	
		}
	}
}else if(result == &amp;quot;acotado&amp;quot;){
	FConfiguracion2.cargarTitulosReportePedido(datos : datos, out datos : datos);
	Integer[] solicitudes = {};
	
	foreach (objeto in this.form.listaPedidos){
		FConfiguracion2.cargarContenidoReportesPedido(pedido : objeto.objeto, datos : datos, out datos : datos);
	}
	
}else if(result == &amp;quot;completo&amp;quot;){
	FConfiguracion2.cargarTitulosReportesPedidoCompleto(datos : datos, out datos : datos);
	
	foreach (objeto in this.form.listaPedidos){
		FConfiguracion2.cargarContenidoReportesPedidoCompleto(pedido : objeto.objeto, datos : datos, out datos : datos);
	}
	
}

Excel.lineasDeTitulos = 1;
ByteArrayOutputStream xlsOutput = new ByteArrayOutputStream();
Excel.crearExcelBinario(buffer : xlsOutput, cantidadDeHojas : 1);
Excel.cargarDatos(datos : (Any[][])datos, nroHoja : 0, nombreHoja : &amp;quot;ListadoPedidos&amp;quot;);
Excel.grabar();

this.fileBinary = xlsOutput.toByteArray();
xlsOutput.close();

this.fileName = &amp;quot;ListadoPedidos_&amp;quot; + &amp;apos;now&amp;apos;.format(mask : &amp;quot;yyyyMMddHHmmss&amp;quot;)+&amp;quot;.xls&amp;quot;;
&lt;/sourceCode&gt;
