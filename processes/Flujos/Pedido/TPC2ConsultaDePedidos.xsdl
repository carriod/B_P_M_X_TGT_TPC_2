				language=&quot;Java1&quot;&gt;Respuesta respuesta;
this.form.listaPedidos.clear();

respuesta = FPedido2.getValuesForFiltros(montoHasta : this.form.montoHasta.monto,
										montoDesde : this.form.montoDesde.monto,
										pedido : this.form.objeto,
										compradorId : this.form.listaComprador, 
                             			jefaturaId : this.form.listaJefatura,
                             			gerenciaId : this.form.listaGerencia,
                             			titulo : this.form.titulo,
                             			fechaInicioDesde : this.form.fechaInicioVigenciaDesde, 
                             			fechaInicioHasta : this.form.fechaInicioVigenciaHasta,
                             			fechaFinDesde : this.form.fechaFinVigenciaDesde,
                             			fechaFinHasta : this.form.fechaFinVigenciaHasta);
if(respuesta.resultado == OK){	
	for(NPedido2 pedido: (NPedido2[])respuesta.contenido){
		this.form.listaPedidos.extend(objeto : pedido);
	}
}else{
    Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
	throw Java.Lang.Exception((String)respuesta.contenido);
}

//BOTONES DE ACCION
this.form.botonesAccion.clear();
this.form.botonesAccion.extend(labelBoton : &amp;quot;Cancelar&amp;quot;);
this.form.botonesAccion.extend(labelBoton : &amp;quot;Volver&amp;quot;);
this.form.botonesAccion.extend(labelBoton : null);
this.form.botonesAccion.extend(labelBoton : null);
this.form.botonesAccion.extend(labelBoton : null);
this.form.botonesAccion.extend(labelBoton : &amp;quot;Cartas&amp;quot;);
this.form.botonesAccion.extend(labelBoton : &amp;quot;Completo&amp;quot;);
this.form.botonesAccion.extend(labelBoton : &amp;quot;Acotado&amp;quot;);&lt;/sourceCode&gt;
