			&lt;sourceCode
				language=&quot;Java1&quot;&gt;this.error = false;

if(this.form == null || result == &amp;quot;limpiar&amp;quot;){
	this.form = Formulario2.FormConsultaPedidos();

	this.form.userID = Participant.id;
	this.form.rol = this.rol;
	
	
		if(this.form.rol == &amp;quot;TPC - Gerente Compras&amp;quot; || this.form.rol == &amp;quot;TPC - Jefe Compras&amp;quot; || this.form.rol == &amp;quot;TPC - Comprador GC&amp;quot;)
		{
			Respuesta respuesta;
			if(this.form.rol == &amp;quot;TPC - Gerente Compras&amp;quot;)
			{
				respuesta = FGerencia2.getValueForUserId(userId : this.form.userID);
				if(respuesta.resultado == OK)	
				{	
					NGerencia2 gerencia = (NGerencia2) respuesta.contenido;
					this.form.listaGerencia = gerencia.id;
				}else{
			    	Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
					this.mensajeError =(String)respuesta.contenido;
					this.error = true;
				}
			}
			else if(this.form.rol == &amp;quot;TPC - Jefe Compras&amp;quot;)
			{
				respuesta = FJefatura2.getValueForUserId(userId : this.form.userID);
				if(respuesta.resultado == OK)	
				{	
					NJefatura2 jefatura = (NJefatura2) respuesta.contenido;
					this.form.listaJefatura = jefatura.id;
				}else{
			    	Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
					this.mensajeError =(String)respuesta.contenido;
					this.error = true;
				}
			}
			else if(this.form.rol == &amp;quot;TPC - Comprador GC&amp;quot;)
			{
				respuesta = FComprador2.getValueForUserId(userId : this.form.userID);
				if(respuesta.resultado == OK)	
				{	
					NComprador2 comprador = (NComprador2) respuesta.contenido;
					this.form.listaComprador = comprador.id;
				}else{
			    	Utiles.Logger.logFatal(mensaje : (String)respuesta.contenido);
					this.mensajeError =(String)respuesta.contenido;
					this.error = true;
				}
		
			}
		}
	
}

this.form.botonesAccion.clear();
Fachada2.FConfiguracion2.setBotones(actividad : this.actividad, rol : this.rol, out botonesAccion : botonesAccion);                      			
for(int index = 0; index &amp;lt; botonesAccion.length; index++){
    this.form.botonesAccion.extend(labelBoton : botonesAccion[index]);
}&lt;/sourceCode&gt;
